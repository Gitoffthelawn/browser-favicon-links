{
  "manifest_version": 2,
  "version": "1.0",
  "name": "Favicons for links",
  "permissions": [
    "webRequest",
    "storage"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "favicon-link@joethei.xyz"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
