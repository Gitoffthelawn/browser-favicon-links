{
  "name": "browser-favicon-links",
  "version": "0.1.0",
  "description": "Browser addon to show the favicon of a linked page",
  "targets": {
    "webext-dev": {
      "sourceMap": {
        "inline": true,
        "inlineSources": true
      }
    },
    "webext-prod": {}
  },
  "devDependencies": {
    "@parcel/config-webextension": "^2.0.1",
    "parcel": "^2.0.1",
    "web-ext": "^6.6.0",
    "webextension-polyfill": "^0.8.0"
  },
  "scripts": {
    "build": "parcel build manifest.json --config @parcel/config-webextension --target webext-prod",
    "package": "cd dist/webext-prod && zip -r favicon_for_links.zip ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joethei/browser-favicon-links.git"
  },
  "author": "Johannes Theiner",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/joethei/browser-favicon-links/issues"
  },
  "homepage": "https://github.com/joethei/browser-favicon-links#readme"
}
