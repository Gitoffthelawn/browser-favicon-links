{
  "name": "Link Favicons",
  "version": "0.1.0",
  "description": "",
  "homepage_url": "https://github.com/joethei/browser-favicon-links",
  "manifest_version": 2,
  "minimum_chrome_version": "74",
  "applications": {
	"gecko": {
	  "id": "favicon-link@joethei.xyz",
	  "strict_min_version": "67.0"
	}
  },
  "browser_specific_settings": {
	"gecko": {
	  "id": "favicon-link@joethei.xyz"
	}
  },
  "permissions": [
	"storage",
	"webRequest"
  ],
  "options_ui": {
	"chrome_style": true,
	"page": "options.html"
  },
  "content_scripts": [
	{
	  "matches": [
		"*://*/*"
	  ],
	  "js": [
		"main.js"
	  ]
	}
  ]
}
