{
  "name": "Link Favicons",
  "version": "0.1.8",
  "description": "See the favicon for a linked website.",
  "homepage_url": "https://github.com/joethei/browser-link-favicons",
  "manifest_version": 2,
  "minimum_chrome_version": "74",
  "browser_specific_settings": {
	"gecko": {
	  "id": "favicon-link@joethei.xyz"
	}
  },
  "icons": {
	"128": "icon.png"
  },
  "permissions": [
	"storage"
  ],
  "options_ui": {
	"chrome_style": true,
	"page": "options.html"
  },
  "content_scripts": [
	{
	  "matches": [
		"*://*/*"
	  ],
	  "js": [
		"main.js"
	  ],
	  "css": [
		"main.css"
	  ]
	}
  ]
}
